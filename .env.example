# Data Source API Keys Configuration
# Copy this file to .env and add your API keys

# Yahoo Finance (no API key needed - default source)
# Free and unlimited, but can be unreliable

# Twelve Data API (FREE: 800 requests/day)
# Get your API key from: https://twelvedata.com/
TWELVE_DATA_API_KEY=your_twelvedata_api_key_here

# Alpha Vantage API (FREE: 25 requests/day)
# Get your API key from: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_alphavantage_api_key_here

# Data Source Priority (comma-separated)
# Options: yfinance, twelvedata, alphavantage
# The system will try sources in this order
DATA_SOURCE_PRIORITY=yfinance,twelvedata,alphavantage

# Enable/Disable specific sources
ENABLE_YFINANCE=true
ENABLE_TWELVE_DATA=true
ENABLE_ALPHA_VANTAGE=true
